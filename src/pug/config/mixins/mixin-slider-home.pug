mixin sliderHome(imgdesk, imgMob)
    a(href="registrarse.html"): div(class='tns-single__content')
        picture 
            source(media="(min-width: 640px)", srcset=`${imgdesk}`, alt="image slider" width="1920" height="600")
            img(class='tns-single__img', src=`${imgMob}`, alt="image slider" width="1920" height="600")
-for (var i = 0; i < sliderHome.length; i++)
    -var d = sliderHome[i]
    +sliderHome(d.imgdesk, d.imgMob)
    